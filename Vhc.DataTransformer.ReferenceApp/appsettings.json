{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "Databases": [
    {
      "name": "DUMMY",
      "connectionString": "Data Source=dummy.db"
    }
  ],
  "Transformations": {
    "UseCloudFilesystem": "false",
    "BucketName": "./Jobs",
    "FilePattern": "*.job.yaml",
    "ConcurrentNumberOfJobs": "-1"
  },
}